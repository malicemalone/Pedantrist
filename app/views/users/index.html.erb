<%= button_to "Logout", {:controller => 'users', :action => 'destroy', :id => session[:user_id]}, {:method => :delete}%>

<h1> Profile for <%=@user.username%></h1>

<ul>
<%=Entry.where(user_id: session[:user_id]).each do |entry|%>
	<li><%=entry.title%></li>
	<li><%=entry.description%></li>
<%end%>


</ul>